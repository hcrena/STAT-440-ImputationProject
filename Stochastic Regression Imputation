#https://statisticsglobe.com/regression-imputation-stochastic-vs-deterministic/
#setwd("C:/Users/hca126/Desktop/project")
setwd("/Users/renachoi/Desktop/STAT 440/project2/data")
train <- read.csv("train.csv")
test <- read.csv("test.csv")
valid <- read.csv("valid.csv")

#install.packages("mice")
library(mice)

#Stochastic regression imputation
imp_sto <- mice(train, method = "norm.nob", m = 1) # Impute data
data_sto <- complete(imp_sto) # Store data
any(is.na(data_sto))

#remove the na if it is still missing after imputation
data_sto$V07[is.na(data_sto$V07)]= 0
data_sto$V08[is.na(data_sto$V08)]= 0
any(is.na(data_det))


#evaluate the model - stochastic model
for(i in 1:dim(valid)[1]){
  row = valid[i,1]
  col = valid[i,2]
  value = data_sto[row,col]
}
mae = mean(abs(value - valid[,3]))
mae

# MAE returns approx 0.63

#impute the result into the test set
for(i in 1:dim(test)[1]){
  row = test[i,1]
  col= test[i,2]
  value = data_sto[row,col]
  test[i,3] = value
  #cat(sprintf("%d:%s,%f\n",row,col,value))
}

#stochastic model is better than the deterministic regredssion imputation when evaluated onto the valid set
